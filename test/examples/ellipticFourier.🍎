λxs.λys.λN.
  { tieSelf ← [({.x)⊳x]; Δ ← ((-)\~) ∴ tieSelf
  ; dxs ⟜ Δ xs; dys ⟜ Δ ys
  ; dts ⟜ [√(x^2+y^2)]`dxs dys
  ; dxss ⟜ (%)`dxs dts; dyss ⟜ (%)`dys dts
  ; pxs ← (+)Λ dxs; pys ← (+)Λ dys; pts ⟜ (+)Λₒ 0 dts; T ⟜}. pts
  ; coeffs ← λn.
    { n ⟜ ℝn; k ⟜ 2*n*𝜋%T
    ; c ⟜ T%(2*n^2*𝜋^2)
    ; cosDiffs ⟜ (-)\~(cos. ∴ (k*)'pts)
    ; aₙ ← c*dxss⋅cosDiffs
    ; cₙ ← c*dyss⋅cosDiffs
    ; sinDiffs ⟜ (-)\~(sin. ∴ (k*)'pts)
    ; bₙ ← c*dxss⋅sinDiffs
    ; dₙ ← c*dyss⋅sinDiffs
    ; (aₙ,bₙ,cₙ,dₙ)
    }
  ; dtss ⟜ (-)\~((^2)'pts)
  ; ppts ⟜ {: pts
  ; 𝜉 ← (-)`pxs ((*)`((%)`dxs dts) ppts)
  ; 𝛿 ← (-)`pys ((*)`((%)`dys dts) ppts)
  ; A ← (½*((%)`dxs dts)⋅dtss + 𝜉⋅dts)%T
  ; C ← (½*((%)`dys dts)⋅dtss + 𝛿⋅dts)%T
  ; (coeffs'irange 1 N 1,A,C)
  }
