λx.λν.
{
  gammaln ←
    λz.
    {
      zz ⟜ z-1;
      c0 ← 0.999999999999997092;
      𝛾 ← 607%128;
      coeffs ← ⟨ 57.1562356658629235
              , _59.5979603554754912
              , 14.1360979747417471
              , _0.491913816097620199
              , 0.339946499848118887e-4
              , 0.465236289270485756e-4
              , _0.983744753048795646e-4
              , 0.158088703224912494e-3
              , _0.210264441724104883e-3
              , 0.217439618115212643e-3
              , _0.164318106536763890e-3
              , 0.844182239838527433e-4
              , _0.261908384015814087e-4
              , 0.368991826595316234e-5
              ⟩;
      -- TODO: maybe faster w/ frange? (segfaults rip)
      ss ← (+)/1 0 ([y%(zz+itof x)] ` (irange 1 14 1) coeffs);
      (((zz+0.5)*(_.(zz+𝛾+0.5)))-(zz+𝛾+0.5))+_.((√(2*𝜋))*(c0+ss))
    };
  f21 ← λz.
  {
    rf ← [(*)/1 1 (frange x (x+y-1) (⌊y))];
    fact ← [(*)/1 1 (frange 1 x (⌊x))];
    ix ← irange 0 99 1;
    -- a₂
    -- a2 ⟜ 1.5*(1.5*(ν+1));
    mkIx ← [{xx⟜itof x;((z^x)*(rf 0.5 xx)*(rf (1.5*(ν+1)) xx))%((fact xx)*(rf 1.5 xx))}];
    (+)/1 0 (mkIx'1 ix)
  };
  -- TODO: PARENTHESES!!
  0.5+f21 ((_x)^2%ν)
}
