-- A0 and C0 in Kuhl+Giardina
λxs.λys.
  { sum ← [(+)/1 0 x]
  ; tieSelf ← [({.x) ⊳ x];Δ ← [(-)\~(tieSelf x)]
  ; dxs ⟜ Δ xs; dys ⟜ Δ ys
  ; dts ⟜ [√(x^2+y^2)]`dxs dys
  ; pxs ← (+)Λ 0 dxs; pys ← (+)Λ 0 dys; pts ⟜ (+)Λ 0 dts
  ; dtss ⟜ 0<|(-)\~((^2)'1 pts)
  ; T ⟜}. pts
  ; 𝜉 ← (-)`pxs ((*)`((%)`dxs dts) pts)
  ; 𝛿 ← (-)`pys ((*)`((%)`dys dts) pts)
  ; A ← ((sum ((*)`((%)`dxs dts) dtss))%2 + (sum ((*)`𝜉 dts)))%T
  ; C ← ((sum ((*)`((%)`dys dts) dtss))%2 + (sum ((*)`𝛿 dts)))%T
  ; (A,C)
  }
