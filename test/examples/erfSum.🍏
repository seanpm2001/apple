λz.
{
  Σ ← λn.λN.λf. (+)/1 0 (f'1 (irange n N 0));
  exp ← λN.λx.
    {
      fact ← [(*)/1 1 (frange 1 x (⌊x))];
      mkIx ← λn. (x^n)%(fact (𝑖n));
      Σ 0 N mkIx
    };
  f11 ← λz.
  {
    rf ← [(*)/1 1 (frange x (x+y-1) (⌊y))];
    mkIx ← [(z^x)%(rf 1.5 (itof x))];
    Σ 0 99 mkIx
  };
  z2 ⟜ z^2;
  2*z*(exp 20 (_(z2)))*(f11 (z2))%(√𝜋)
}
