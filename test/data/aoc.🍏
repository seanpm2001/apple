{
  A ← ⟨⟨7,6,4,2,1⟩
      ,⟨1,2,7,8,9⟩
      ,⟨9,7,6,2,1⟩
      ,⟨1,3,2,4,5⟩
      ,⟨8,6,4,4,1⟩
      ,⟨1,3,6,7,9::𝟘⟩
      ⟩;
  all ← λp.λxs. (∧)/ₒ #t (p'xs);
  btoi ← [?x,.1::𝟘,.0];
  clamp ← λlb.λub. [x≥lb∧x≤ub];
  count ← λas. (+)/(btoi'as);
  count ((λas. all (clamp 1 3) as ∨ all (clamp _3 _1) as)'((-)\~)'A)
}
